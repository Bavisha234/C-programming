#include <stdio.h>
#include <math.h>

int main() {
    float a;
    int b, c, d;

   
    scanf("%f", &a);         
    scanf("%d", &b);      
    scanf("%d", &c);         
    scanf("%d", &d);         

  
    printf("%.0f\n", floor(a));     // Floor of a
    printf("%.0f\n", ceil(a));     
    printf("%.0f\n", sqrt(b));      
    printf("%.0f\n", pow(c, d));    

    return 0;
}
